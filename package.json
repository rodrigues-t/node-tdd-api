{
  "name": "node-tdd-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "test": "jest --silent --noStackTrace",
    "test:unit": "npm test -- --watch -c jest.unit.config.ts",
    "test:integration": "npm test -- --watch -c jest.integration.config.ts",
    "test:staged": "npm test -- --passWithNoTests --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodrigues-t/node-tdd-api.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rodrigues-t/node-tdd-api/issues"
  },
  "homepage": "https://github.com/rodrigues-t/node-tdd-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/validator": "^13.7.1",
    "bcrypt": "^5.0.1",
    "express": "^4.17.2",
    "jest-mock": "^27.5.0",
    "ts-jest": "^27.1.3",
    "validator": "^13.7.0"
  }
}
